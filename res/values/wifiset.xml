        <item name="android:textColor">@color/gray99_color</item>
    </style>

    <style name="wifiset_imageViewremind">
        <item name="android:layout_width">10dip</item>
        <item name="android:layout_height">10dip</item>
        <item name="android:layout_marginLeft">10dip</item>
        <item name="android:layout_marginRight">5dip</item>
        <item name="android:layout_marginTop">5dip</item>
        <item name="android:background">@drawable/listinfo_bg</item>
    </style>

