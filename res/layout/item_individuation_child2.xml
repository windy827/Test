<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/wholebackground"
    android:orientation="vertical" >

    <RelativeLayout
        android:id="@+id/rl_item_individuation_child2"
        android:layout_width="match_parent"
        android:layout_height="300dip" >

        <RadioGroup
            android:id="@+id/rg_item_individuation_child2"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginRight="80dp"
            android:gravity="center_vertical"
            android:paddingLeft="25dip"
            android:textColor="@color/textColor" >

            <RadioButton
                android:id="@+id/rd_item_individuation_child2_1"
                style="@style/radiobutton_Style"
                android:layout_weight="1"
                android:checked="true"
               
                android:text="RadioButton" />

            <RadioButton
                android:id="@+id/rd_item_individuation_child2_2"
                style="@style/radiobutton_Style"
                android:layout_weight="1"
               
                android:text="RadioButton" />

            <RadioButton
                android:id="@+id/rd_item_individuation_child2_3"
                style="@style/radiobutton_Style"
                android:layout_weight="1"
                
                android:text="RadioButton" />

            <RadioButton
                android:id="@+id/rd_item_individuation_child2_4"
                style="@style/radiobutton_Style"
                android:layout_weight="1"
                
                android:text="RadioButton" />

            <RadioButton
                android:id="@+id/rd_item_individuation_child2_5"
                style="@style/radiobutton_Style"
                android:layout_weight="1"
                
                android:text="RadioButton" />

            <RadioButton
                android:id="@+id/rd_item_individuation_child2_6"
                style="@style/radiobutton_Style"
                android:layout_weight="1"
                
                android:text="RadioButton" />
        </RadioGroup>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_alignParentRight="true"
            android:layout_alignParentTop="true"
            android:orientation="vertical" >

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:gravity="center"
                android:orientation="vertical" >

                <ImageView
                    android:id="@+id/iv_item_individuation_sound1"
                    android:layout_width="@dimen/individuation_iv_sound_widthAndheight"
                    android:layout_height="@dimen/individuation_iv_sound_widthAndheight"
                    android:layout_gravity="center|right"
                    android:layout_marginBottom="2dip"
                    android:layout_marginRight="26dp"
                    android:background="@drawable/listen_nor" />

                <include
                    style="@style/horDiver_withMarginStyle"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    layout="@layout/model_diver_hor_whitebg" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:gravity="center"
                android:orientation="vertical" >

                <ImageView
                    android:id="@+id/iv_item_individuation_sound2"
                    android:layout_width="@dimen/individuation_iv_sound_widthAndheight"
                    android:layout_height="@dimen/individuation_iv_sound_widthAndheight"
                    android:layout_gravity="center|right"
                    android:layout_marginBottom="2dip"
                    android:layout_marginRight="26dp"
                    android:background="@drawable/listen_nor" />

                <include
                    style="@style/horDiver_withMarginStyle"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    layout="@layout/model_diver_hor_whitebg" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:gravity="center"
                android:orientation="vertical" >

                <ImageView
                    android:id="@+id/iv_item_individuation_sound3"
                    android:layout_width="@dimen/individuation_iv_sound_widthAndheight"
                    android:layout_height="@dimen/individuation_iv_sound_widthAndheight"
                    android:layout_gravity="center|right"
                    android:layout_marginBottom="2dip"
                    android:layout_marginRight="26dp"
                    android:background="@drawable/listen_nor" />

                <include
                    style="@style/horDiver_withMarginStyle"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    layout="@layout/model_diver_hor_whitebg" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:gravity="center"
                android:orientation="vertical" >

                <ImageView
                    android:id="@+id/iv_item_individuation_sound4"
                    android:layout_width="@dimen/individuation_iv_sound_widthAndheight"
                    android:layout_height="@dimen/individuation_iv_sound_widthAndheight"
                    android:layout_gravity="center|right"
                    android:layout_marginBottom="2dip"
                    android:layout_marginRight="26dp"
                    android:background="@drawable/listen_nor" />

                <include
                    style="@style/horDiver_withMarginStyle"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    layout="@layout/model_diver_hor_whitebg" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:gravity="center"
                android:orientation="vertical" >

                <ImageView
                    android:id="@+id/iv_item_individuation_sound5"
                    android:layout_width="@dimen/individuation_iv_sound_widthAndheight"
                    android:layout_height="@dimen/individuation_iv_sound_widthAndheight"
                    android:layout_gravity="center|right"
                    android:layout_marginBottom="2dip"
                    android:layout_marginRight="26dp"
                    android:background="@drawable/listen_nor" />

                <include
                    style="@style/horDiver_withMarginStyle"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    layout="@layout/model_diver_hor_whitebg" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:gravity="center"
                android:orientation="vertical" >

                <ImageView
                    android:id="@+id/iv_item_individuation_sound6"
                    android:layout_width="@dimen/individuation_iv_sound_widthAndheight"
                    android:layout_height="@dimen/individuation_iv_sound_widthAndheight"
                    android:layout_gravity="center|right"
                    android:layout_marginBottom="2dip"
                    android:layout_marginRight="26dp"
                    android:background="@drawable/listen_nor" />

                <include
                    style="@style/horDiver_withMarginStyle"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    layout="@layout/model_diver_hor_whitebg" />
            </LinearLayout>
        </LinearLayout>
    </RelativeLayout>

</LinearLayout>